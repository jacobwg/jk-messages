<div class="row" id="more-container">
  <div class="span12">
    <a class="btn btn-large btn-success" id="load-more">Load More Messages</a>
  </div>
</div>

<div id="content">
  <%= render(@messages) %>
</div>

<% if @messages.size > 0 %>
<script type="text/javascript">
  window.since = '<%= @latest.time_cst.iso8601 %>';
</script>
<% end %>
