<% if @since %>
  <% if @messages.size > 0 %>
  $("#content").append("<%= escape_javascript(render(@messages)) %>");
  window.since = '<%= @messages.last.time_cst.iso8601 %>';
  <% end %>
<% else %>
  <% if @messages.size > 0 %>
  $("#content").prepend("<%= escape_javascript(render(@messages)) %>");
  <% else  %>
  window.done = true;
  <% end %>
<% end %>